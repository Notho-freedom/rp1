Lable(f,font=("Lucia calligraphy",15,"bold"),text="Dosa........Rs.60/plate",fg="black",bg="lightgreen").place(x=10,y=80)
Lable(f,font=("Lucia calligraphy",15,"bold"),text="Dosa........Rs.60/plate",fg="black",bg="lightgreen").place(x=10,y=80)
Lable(f,font=("Lucia calligraphy",15,"bold"),text="Dosa........Rs.60/plate",fg="black",bg="lightgreen").place(x=10,y=80)
Lable(f,font=("Lucia calligraphy",15,"bold"),text="Dosa........Rs.60/plate",fg="black",bg="lightgreen").place(x=10,y=80)

root.mainloop()